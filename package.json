{
    "name": "greater-bengaluru-info",
    "version": "1.0.0",
    "scripts": {
        "build": "eleventy && npx tailwindcss -i src/css/style.css -o _site/src/css/style.css --minify",
        "serve": "eleventy --serve & npx tailwindcss -i src/css/style.css -o _site/src/css/style.css --watch",
        "dev": "npm run serve",
        "cms": "npx netlify-cms-proxy-server"
    },
    "dependencies": {
      "@11ty/eleventy": "^3.0.0",
      "@11ty/eleventy-img": "*",
      "tailwindcss": "*",
      "@tailwindcss/typography": "*",
      "postcss": "*",
      "autoprefixer": "*"
    }
}
